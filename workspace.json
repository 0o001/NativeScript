      "architect": {
        "ios": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "tns debug ios --no-hmr --emulator"
              }
            ],
            "cwd": "packages/core/__tests__/e2e/automated",
            "parallel": false
          }
