            "parallel": false
          }
        },
        "clean": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx rimraf -- hooks node_modules platforms package-lock.json webpack.config.js"
              },
              {
                "command": "npm i"
              },
              {
                "command": "npx rimraf -- package-lock.json"
              }
